data Plus(+a : CBV, +b : CBV) : CBV {
    Left(a),
    Right(b)
};

data Tensor(+a : CBV, +b : CBV) : CBV {
    MkTensor(a, b)
};

codata With(+a : CBV, +b : CBV) : CBN {
    Left[a],
    Right[a]
};

codata Par(+a : CBV, +b : CBV) : CBN {
    MkPar[a,b]
};

data Not(-a : CBV) : CBV {
    MkNot[a]
};

data PosBot : CBV {};
