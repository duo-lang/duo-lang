import Prelude;

destructor Ap : CBV;

prd rec addS := cocase { Ap(x,y)[k] =>
                       x
                       >>
                       case { Z    => y >> k
                             , S(z) => addS >> Ap(z,S(y))[k]
                             }
                   };

prd rec addA := \x => \y => case x of { Z => y
                                      , S(z) => addA z S(y)
					        };
		     


cmd main := Read[ mu x. Read[mu y. Print(addA x y);Done]];
