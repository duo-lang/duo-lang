import Function;
import I64;
import F64;

def prd x : I64 := MkI64(7#I64);
def prd y : I64 := MkI64(2#I64);

def prd xf : F64 := MkF64(7.0#F64);
def prd yf : F64 := MkF64(2.0#F64);

def cmd main :=
    Print(add x y);
    Print(sub x y);
    Print(div x y);
    Print(mul x y);
    Print(mod x y);
    ExitSuccess;

def cmd mainf :=
    Print(addf xf yf);
    Print(subf xf yf);
    Print(divf xf yf);
    Print(mulf xf yf);
    ExitSuccess;
