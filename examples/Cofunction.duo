data CoFun : (-f : CBV, +e : CBV) -> CBV {
    CoAp(return f,e)
};

type operator -< rightassoc at 0 := CoFun;

def cns id : forall a. a -< a  :=
  case { CoAp(k,e) => e >> k };

def cns id2 : forall a. a -< a := \x =< x; 

def cns compose : forall a b c.(b -< c) -< (c -< a) -< b -< a :=
  \f g x =< g (f x);

  

