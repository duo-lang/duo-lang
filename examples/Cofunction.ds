data CoFun : (-f : CBV, +e : CBV) -> CBV {
    CoAp(e,return f)
};

type operator -< rightassoc at 0 := CoFun;


def cns id : forall a. a -< a  :=
  case { CoAp(e,f) => e >>f };

def cns id2 : forall a. a -< a := \f =< f; 

def cns compose : forall a b c . (a -< b) -<  (b -< c) -< (a -< c) :=
  \f g x =< g (f x);

  

