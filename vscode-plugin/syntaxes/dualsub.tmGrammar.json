{
    "scopeName": "source.dualsub",
    "patterns": [
        { "include": "#keywords" },
        { "include": "#symbols" },
        { "include": "#numbers" },
        { "include": "#comments" },
        { "include": "#brackets" }
    ],
    "repository": {
      "keywords": {
        "patterns": [
            {
              "match": "\\b(match|case|cocase|comatch|prd|cns|cmd|with|of|Done|Print|forall|data|codata|rec|mu|import|set|Top|Bot|Type|CBV|CBN)\\b",
              "name": "keyword.control"
            }
          ]

      },
      "symbols": {
        "patterns": [{
            "match": "(:=|=>|>>|\\\\/|/\\\\|<:|<<:|:>>)", 
            "name": "keyword.syntax"
        }]

      },
      "numbers": {
        "patterns": [{
            "match": "\\b[0-9]+",
            "name": "constant.numeric"
          }]
      },
      "comments": {
        "patterns": [
            {
              "match": "#.*$\n?",
              "name": "comment.line.number-sign.syntax"
            }
          ]
      },
      "brackets": {
        "patterns": [
          {
            "match": "[\\[\\]{\\(}\\)]",
            "name": "punctuation.section.parens"
          }
        ]
      }
    }
}
