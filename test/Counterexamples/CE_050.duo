module CE_050;

-- make  sure idLRec is not typechecked as List(⊤) -> List(⊥)
import Codata.Function;
import Data.List;
import Data.Bool;
import Data.Unit;

def rec prd idLRec := \l => case l of {
    Nil => Nil,
    Cons(x,xs) => Cons(x,idLRec xs)
};

def prd fail : List@(Bool) := Cons(True, idLRec Cons(MkUnit,Nil));
