module CE_049;

-- Type checking in instance definition fails.
import Bool;
import List;

class Eq(+a : CBV) {
  Equals(a, a, return Bool),
  Differ(a, a, return Bool)
};

instance Eq Bool {
  Equals(x, y, k) => or (and x y) (not (or x y)) >> k,
  Differ(x, y, k) => Cons((or x y), (not (and x y))) >> k
};